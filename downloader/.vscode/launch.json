{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Script Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "exec",
                "${env:userprofile}/.dotnet/tools/.store/dotnet-script/0.28.0/dotnet-script/0.28.0/tools/netcoreapp2.1/any/dotnet-script.dll",
                "main.csx",
                "https://raw.githubusercontent.com/angular/angular/master/README.md",
                // "-r",
                // "temp-remote-proxy",
                // "-o",
                // "test.txt"
            ],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": true
        }
    ]
}